{
  "name": "cloud-gaming-fps",
  "version": "0.0.5",
  "description": "Measures the FPS of the video element",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kishore180994/CloudGamingFPS.git"
  },
  "scripts": {
    "cssbuild": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --minify",
    "build": "tsc --noEmit && vite build && yarn cssbuild",
    "build:watch": "cross-env __DEV__=true vite build --watch && yarn cssbuild",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@react-pdf/renderer": "^3.1.9",
    "@reduxjs/toolkit": "^1.9.5",
