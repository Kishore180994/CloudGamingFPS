    "jspdf": "^2.5.1",
    "lodash.debounce": "^4.0.8",
    "pdfmake": "^0.2.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.5",
    "react-redux": "^8.0.5",
    "recharts": "^2.6.2",
    "redux-batched-subscribe": "^0.1.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.0.0-rc.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
